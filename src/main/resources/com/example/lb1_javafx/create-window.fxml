<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.lb1_javafx.fxController.CreateWindow">
   <children>
      <TextField fx:id="title" layoutX="59.0" layoutY="73.0" promptText="string" />
      <TextField fx:id="count" layoutX="59.0" layoutY="213.0" promptText="double" />
      <TextField fx:id="price" layoutX="59.0" layoutY="279.0" text="int" />
      <Text layoutX="59.0" layoutY="69.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Title" />
      <Text layoutX="59.0" layoutY="142.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Release date" />
      <Text layoutX="59.0" layoutY="209.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Count" />
      <Text layoutX="59.0" layoutY="275.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Price" />
      <ChoiceBox fx:id="gameStatus" layoutX="59.0" layoutY="332.0" prefWidth="150.0" />
      <Text layoutX="59.0" layoutY="328.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Game status" />
      <Button fx:id="createButton" layoutX="249.0" layoutY="332.0" mnemonicParsing="false" onAction="#validate" text="Create" />
      <DatePicker fx:id="releaseDate" editable="false" layoutX="59.0" layoutY="146.0" promptText="date" />
      <Button fx:id="backButton" layoutX="325.0" layoutY="332.0" mnemonicParsing="false" onAction="#openMain" text="Back" />
   </children>
</AnchorPane>
