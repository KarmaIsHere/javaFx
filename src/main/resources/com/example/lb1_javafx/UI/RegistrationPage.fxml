<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="800.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.lab1.Controllers.RegistrationPage">
   <children>
      <Button fx:id="NewMemberSignUp" layoutX="300.0" layoutY="529.0" mnemonicParsing="false" onAction="#Validate" text="Sign Up" />
      <TextField fx:id="NewName" layoutX="213.0" layoutY="189.0" promptText="Name" />
      <TextField fx:id="NewSurname" layoutX="213.0" layoutY="231.0" promptText="Surname" />
      <TextField fx:id="NewphNumber" layoutX="213.0" layoutY="270.0" promptText="Phone Number (86...)" />
      <TextField fx:id="NewUsername" layoutX="213.0" layoutY="309.0" promptText="Username" />
      <PasswordField fx:id="NewPassword" layoutX="213.0" layoutY="351.0" promptText="Password" />
      <Text layoutX="148.0" layoutY="129.0" strokeType="OUTSIDE" strokeWidth="0.0" text="REGISTER">
         <font>
            <Font size="63.0" />
         </font>
      </Text>
      <Button fx:id="ReturnBtn" layoutX="213.0" layoutY="529.0" mnemonicParsing="false" onAction="#LogInReturn" text="Return" />
      <ChoiceBox fx:id="ChoiceBox" layoutX="214.0" layoutY="433.0" prefWidth="150.0" />
      <Button fx:id="FileChooser" layoutX="214.0" layoutY="475.0" mnemonicParsing="false" onAction="#ChooseFiles" prefHeight="25.0" prefWidth="150.0" text="Choose files" />
      <Label fx:id="fileLabel" layoutX="199.0" layoutY="506.0" prefHeight="17.0" prefWidth="390.0" />
      <DatePicker fx:id="MyDatePicker" layoutX="213.0" layoutY="388.0" onAction="#GetDate" prefHeight="25.0" prefWidth="149.0" promptText="Enter a date" />
   </children>
</AnchorPane>
